{
    "version": 2,
    "builds": [
        
      {
        "src": "pyproject.toml",
        "use": "@vercel/python",
        "config": { "installCommand": "poetry install --no-root" }
      },
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "trailingSlash": true
}
